{
    "ruleset": "Possession",
    "comments": [
        "pg112 from OGTS"
    ],
    "type": "re",
    "flags": "I",
    "rules": [
        {
            "comments": [
                "Use an apostrophe after plural nouns ending in s."
            ],
            "re": "\\w+s's"
        },
        {
            "comments": [
                "Do not use the apostrophe in the possessive pronouns hers, its, ours, yours and theirs."
            ],
            "prefix": "\\b",
            "suffix": "\\b",
            "re": [
                "(her's|hers')",
                "(their's|theirs')",
                "(his's|his')"
            ]
        },
        {
            "name": "It's vs. Its",
            "prefix": "\\b",
            "suffix": "\\b",
            "re": "it's"
        }
    ]
}
